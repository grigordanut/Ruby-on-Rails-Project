<h1>Products for your preference</h1>
<div class="row">
<% @clothing_preference.each do |product| %>
<div class ="col-lg-4">
<div class="card" style="width: 18rem;">
  <img src=<%= product.imageUrl %> class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= product.title %></h5>
    <p class="card-text"><%= product.price %></p>
    <a href=<%= product.detailPageURL %> class="btn btn-primary">Vist Amazon</a>
  </div>
</div>
</div>
<% end %>
</div>
