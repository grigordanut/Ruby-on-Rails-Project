<h1>Search Result</h1>
<div class="row">
<% @apiproducts.response.docs.each do |product| %>
<div class ="col-lg-4">
<div class="card" style="width: 18rem;">
  <img src=<%= product.thumb_image %> class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= product.title %></h5>
    <h5 class="card-title">Price <%= product.price %></h5>
    <h5 class="card-title">Sale Price<%= product.sale_price %></h5>
    <p class="card-text"><%= product.brand %></p>
    <a href=<%= product.url %> class="btn btn-primary">Vist Website</a>
  </div>
</div>
</div>
<% end %>
</div>
